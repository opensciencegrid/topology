#!/usr/bin/env python3
# -*- coding: utf-8 -*-

"""
A script to generate Authfiles for XCaches in the OSG
"""

import webapp.models
import stashcache

global_data = webapp.models.GlobalData()

VOData = global_data.get_vos_data()

authfile = stashcache.generate_authfile(None, VOData)

print(authfile)
